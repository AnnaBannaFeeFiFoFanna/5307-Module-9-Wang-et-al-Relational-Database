DROP TABLE IF EXISTS Delay_Cause_Op_Carrier CASCADE;
DROP TABLE IF EXISTS DSCRM_By_Type CASCADE;
DROP TABLE IF EXISTS Airline_Complaints CASCADE;
DROP TABLE IF EXISTS Mishandling_By_Op_Carrier CASCADE;
DROP TABLE IF EXISTS Airline_Arr_Op_Carrier CASCADE;
DROP TABLE IF EXISTS Monthly_Enplanements CASCADE;
DROP TABLE IF EXISTS Timeliness_By_Airport CASCADE;
DROP TABLE IF EXISTS Excessive_Tarmac_Delays CASCADE;
DROP TABLE IF EXISTS Flight_Num_Org_and_Dest CASCADE;
DROP TABLE IF EXISTS Airport_DateTime_ID CASCADE;
DROP TABLE IF EXISTS Airport CASCADE;
DROP TABLE IF EXISTS Airline CASCADE;

CREATE TABLE Airport (
Airport_ID CHAR(3) NOT NULL,
Airport_ZipCode CHAR(5) NOT NULL,
Airport_City VARCHAR(30) NOT NULL,
Airport_State VARCHAR(2) NOT NULL,
CONSTRAINT PKAirport PRIMARY KEY (Airport_ID)
);
